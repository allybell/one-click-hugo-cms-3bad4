{
  "name": "one-click-hugo-cms",
  "version": "1.0.0",
  "description": "Hugo template with sveltiaCMS instead of Decap CMS ",
  "repository": "allybell/one-click-hugo-cms-3bad4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint site/assets cypress",
    "start": "hugo server -d ../dist -s site -w -p 3000 --minify --watch",
    "build": "hugo -d ../dist -s site --minify --cleanDestinationDir",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.21",
    "cypress": "^10.11.0",
    "eslint": "^9.37.0",
    "eslint-plugin-cypress": "^5.2.0",
    "globals": "^16.4.0",
    "netlify-plugin-cypress": "^2.2.1",
    "postcss": "^8.5.6",
    "postcss-autocorrect": "^1.3.2",
    "postcss-cli": "^11.0.1",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.8.3"
  }
